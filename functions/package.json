{
  "name": "astroai-functions",
  "version": "1.0.0",
  "description": "Cloud Functions for AstroAI",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "serve": "npm run build && firebase emulators:start --only functions",
    "shell": "npm run build && firebase functions:shell",
    "start": "npm run shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log"
  },
  "engines": {
    "node": "18"
  },
  "main": "lib/index.js",
  "dependencies": {
    "firebase-admin": "^12.0.0",
    "firebase-functions": "^4.8.0",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "node-fetch": "^3.3.2",
    "openai": "^4.20.1",
    "pinecone-client": "^1.1.0",
    "razorpay": "^2.9.2",
    "stripe": "^14.7.0",
    "puppeteer": "^21.5.2",
    "pdf-lib": "^1.17.1",
    "crypto": "^1.0.1"
  },
  "devDependencies": {
    "typescript": "^4.9.0",
    "@types/node": "^18.0.0",
    "@types/express": "^4.17.17",
    "@types/cors": "^2.8.13",
    "firebase-functions-test": "^3.1.0"
  },
  "private": true
}

